<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="../../css/style.css">
    <title>User List</title>
</head>
<body>
    <div class = "grid">
        <div class = "banner">
            <h1>User List</h1>
        </div>
        <%- include('partials/navbar') %>
        <div class = "content_section">
            <fieldset>
                <legend>User Details From Database: </legend>
                    <table>
                        <tr>
                            <th>Forename</th>
                            <th>Surname</th>
                            <th>Address</th>
                            <th>System Admin?</th>
                            <th>Account Status</th>
                        </tr>
                        <%users.forEach(function(obj){%>
                        <tr>
                            <td>
                                <%=obj.userforename%>
                            </td>
                            <td>
                                <%=obj.usersurname%>
                            </td>
                            <td>
                                <%=obj.userAddress%>
                            </td>
                            <td>
                                <%=obj.executiveAC%>
                            </td>
                            <td>
                                <%=obj.userStatus%>
                            </td>
                        </tr>
                        <%})%>
                    </table>
            </fieldset>
        </div>
    </div>
    
</body>
</html>